{
  "name": "no-name-blog",
  "version": "1.0.0",
  "description": "Blog built with React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production --env.FB_CLIENT=\"$FB_CLIENT_PROD\" && babel ./src -d ./build",
    "build:firebase": "webpack --mode production --env.FB_CLIENT=\"$FB_CLIENT_PROD\" && babel ./src -d ./functions/build",
    "start": "npm run build && NODE_ENV=production node ./build/server/index.js",
    "client": "webpack --mode development --config ./webpack.config.js/ --env.FB_CLIENT=\"$FB_CLIENT_DEV\" --watch",
    "server": "nodemon  --signal SIGINT --exec babel-node src/server/index.js --watch src",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Min Gyo Kim",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "axios": "^0.21.1",
    "babel-loader": "^8.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "css-loader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "file-loader": "^3.0.1",
    "firebase": "^7.15.0",
    "firebase-admin": "^8.12.1",
    "lodash": "^4.17.19",
    "markdown-to-jsx": "^6.11.4",
    "prop-types": "^15.7.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-dropzone": "^11.0.2",
    "react-infinite-scroll-component": "4.5.3",
    "react-redux": "^7.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "serialize-javascript": "^4.0.0",
    "serve-favicon": "^2.5.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "uuidv4": "^6.2.0",
    "webpack": "^4.5.0"
  },
  "devDependencies": {
    "@babel/node": "^7.10.1",
    "babel-eslint": "^10.0.0",
    "concurrently": "^4.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "nodemon": "^1.17.2",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
